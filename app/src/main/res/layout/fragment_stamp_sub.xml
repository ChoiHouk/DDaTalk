<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/diamondwheelmap"
        android:layout_width="123dp"
        android:layout_height="112dp"
        android:layout_marginRight="45dp"
        android:layout_marginTop="45dp"
        android:contentDescription="diamond"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/silverwheelmap"
        tools:ignore="RtlHardcoded" />

    <ImageView
        android:id="@+id/silverwheelmap"
        android:layout_width="123dp"
        android:layout_height="112dp"
        android:layout_marginRight="45dp"
        android:layout_marginTop="45dp"
        android:contentDescription="silver"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/stonewheelmap"
        tools:ignore="RtlHardcoded" />

    <ImageView
        android:id="@+id/goldwheelmap"
        android:layout_width="123dp"
        android:layout_height="112dp"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="45dp"
        android:contentDescription="gold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tirewheelmap"
        tools:ignore="RtlHardcoded" />

    <ImageView
        android:id="@+id/tirewheelmap"
        android:layout_width="123dp"
        android:layout_height="112dp"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="45dp"
        android:contentDescription="tire"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/woodwheelmap"
        tools:ignore="RtlHardcoded" />

    <ImageView
        android:id="@+id/stonewheelmap"
        android:layout_width="123dp"
        android:layout_height="112dp"
        android:layout_marginRight="45dp"
        android:layout_marginTop="60dp"
        android:contentDescription="stone"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="RtlHardcoded" />

    <ImageView
        android:id="@+id/woodwheelmap"
        android:layout_width="123dp"
        android:layout_height="112dp"
        android:layout_marginLeft="45dp"
        android:layout_marginTop="60dp"
        android:contentDescription="wood"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/backtostamp"
        android:layout_width="51dp"
        android:layout_height="55dp"
        android:background="@android:color/transparent"
        app:srcCompat="@drawable/back"
        tools:ignore="MissingConstraints,RtlHardcoded"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent" />
</android.support.constraint.ConstraintLayout>
